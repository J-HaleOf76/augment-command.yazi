;218;49m]\u001b[38;2;159;126;254;49m[augment-section]\u001b[18;86H\u001b[38;2;122;130;218;49m-\u001b[38;2;171;178;191;49m \u001b[38;2;122;130;218;49m[\u001b[38;2;16;177;254;49mAugmented commands\u001b[38;2;122;130;218;49m](\u001b[38;2;16;177;254;49m#augmented-commands\u001b[38;2;122;130;218;49m)\u001b[19;86H-\u001b[38;2;171;178;191;49m \u001b[38;2;122;130;218;49m[\u001b[38;2;16;177;254;49mNew commands\u001b[38;2;122;130;218;49m](\u001b[38;2;16;177;254;49m#new-commands\u001b[38;2;122;130;218;49m)\u001b[20;86H-\u001b[38;2;171;178;191;49m \u001b[38;2;122;130;218;49m[\u001b[38;2;16;177;254;49mUsage\u001b[38;2;122;130;218;49m](\u001b[38;2;16;177;254;49m#usage\u001b[38;2;122;130;218;49m)\u001b[21;86H-\u001b[38;2;171;178;191;49m \u001b[38;2;122;130;218;49m[\u001b[38;2;16;177;254;49mLicence\u001b[38;2;122;130;218;49m](\u001b[38;2;16;177;254;49m#licence\u001b[38;2;122;130;218;49m)\u001b[23;86H\u001b[38;2;249;200;89;49m## Requirements\u001b[25;86H\u001b[38;2;122;130;218;49m-\u001b[38;2;171;178;191;49m \u001b[38;2;122;130;218;49m[\u001b[38;2;16;177;254;49mYazi\u001b[38;2;122;130;218;49m](\u001b[38;2;16;177;254;49mhttps://github.com/sxyazi/yazi\u001b[38;2;122;130;218;49m)\u001b[38;2;171;178;191;49m v0.4.2+\u001b[26;86H\u001b[38;2;122;130;218;49m-\u001b[38;2;171;178;191;49m \u001b[38;2;122;130;218;49m[`\u001b[38;2;249;200;89;49m7z\u001b[38;2;122;130;218;49m`\u001b[38;2;16;177;254;49m or \u001b[38;2;122;130;218;49m`\u001b[38;2;249;200;89;49m7zz\u001b[38;2;122;130;218;49m`\u001b[38;2;16;177;254;49m command\u001b[38;2;122;130;218;49m]\u001b[38;2;159;126;254;49m[7z-link]\u001b[27;86H\u001b[38;2;122;130;218;49m-\u001b[38;2;171;178;191;49m \u001b[38;2;122;130;218;49m[`\u001b[38;2;249;200;89;49mfile\u001b[38;2;122;130;218;49m`\u001b[38;2;16;177;254;49m command\u001b[38;2;122;130;218;49m]\u001b[38;2;159;126;254;49m[file-command-link]\u001b[29;86H\u001b[38;2;249;200;89;49m## Installation\u001b[31;86H\u001b[38;2;122;130;218;49m```\u001b[38;2;159;126;254;49msh\u001b[32;86H\u001b[38;2;99;10